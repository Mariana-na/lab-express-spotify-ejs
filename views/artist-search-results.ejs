<h1>Artists Results</h1>


<% if (results.length === 0) { %>
    <p>No artists found.</p>
<% } else { %>
    <% results.forEach ((artist) => { %>
        <div id = "artist" >
            <h2><%= artist.name %></h2>
            <img src="<%=artist.images.map(element=>element.url).splice(0,1)%>" alt="<%= artist.name %>">
            <a href="/albums/ <%= artist.id %>"> Show Albums</a>
        </div>
    <% } ) %>
<% } %>

